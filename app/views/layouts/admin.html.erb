<!DOCTYPE html>
<html>
<head>
  <title>Artstore 後台</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container-fluid body">
    <%= render "common/navbar" %>
    <%= notice_message %>
    
    <div class="row">
      <div class="col-md-2 removeDefaultPadding">
        <%= render "common/admin_menu" %>
      </div>
      <div class="col-md-10">
        <%= yield %>
      </div>
    </div>
  </div>
</body>
</html>